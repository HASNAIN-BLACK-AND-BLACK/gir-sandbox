            if ($(window).scrollTop() < 100 ){
              $('#kustomer-ui-sdk-iframe').addClass("hide-away");
              console.log("hidden") 
            }
          };
        });
      })
  
      let isChatOpen = null
      window.addEventListener('load', (event) => {
        document.querySelector('#OpenKustomerChat').addEventListener('click', () => {
          Kustomer.open();
        })
        Kustomer.addListener('onOpen', function(){
          isChatOpen = true
        })
        Kustomer.addListener('onClose', function(){
          isChatOpen = false
        })
      })
      // <!-- Hide/Show Kustomer Chat Widget on Scroll -->
      $(document).ready(function(){
        if(window.matchMedia('(max-width: 750px)').matches){
          $(window).scroll(function(event) {
            if (($(window).scrollTop() < 100) && !isChatOpen) {
              $('#kustomer-ui-sdk-iframe').addClass("hide-away"); 
            } else {
              $('#kustomer-ui-sdk-iframe').removeClass("hide-away");
            }
          });
        }
      })

    <script>
      var notificationCallback = function(data) {
        var msg = '';
        if (data.status == 'OK') {
          $('.error-msg').hide();
          $('.success-msg').show();
          setTimeout(function() { 
            $('.bis-custom-form').removeClass('show-popup');
            $('body').removeClass('popup-open');
          }, 3000);
        } else {
          $('.success-msg').hide();
          $('.error-msg').show();
        }
        //console.log(data);
      }
      $('#notify_button').click(function(e) {
        e.preventDefault();
        var email = $('#notify_email').val(),
            productId = {{product.id}}, 
            variantId = $('input[name="id"]').val(); 
  
            var acceptAgreement = document.getElementById("accept_agreement");
            if (acceptAgreement.checked == true) {
              $('.bis-checkbox').removeClass('checkbox-error');
              BIS.create(email, variantId, productId, {accepts_marketing: true}).then(notificationCallback);
            } else {
              $('.bis-checkbox').addClass('checkbox-error');
            }  
      })

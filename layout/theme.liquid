  <script>
    // Adjust search modal position on mobile when scroll
    const searchModalPosition = () => {
      var winWidth = window.innerWidth;
      var currPosition = window.scrollY || window.scrollTop || document.getElementsByTagName("html")[0].scrollTop;
      if (winWidth < 750) {
        if (currPosition > 10 && !document.querySelector(".header__search details[open]>.search-modal")?.classList.contains("sticky")) {
          document.querySelector(".header__search details[open]>.search-modal")?.classList.add("sticky");
        } else if (currPosition == 0) {
          document.querySelector(".header__search details[open]>.search-modal")?.classList.remove("sticky");
        }
      }
    }
    document.addEventListener("scroll", searchModalPosition);
  
  </script>


  <style>
    #pandectes-banner {
      display: none!important;
    }
    #pandectes-banner.pandectes-override-show {
      display: block!important;
    }
  </style>
  <script>
    let firstConsent = false;
    function pandectesClickBinder() {
      window.addEventListener('load', (event) => {
        const el = document.querySelector('a[href="#revokeConsent"]');
        if (el) {
          console.log('Attaching binder');
          el.addEventListener('click', function(){
            const bannerIdEl = document.getElementById('pandectes-banner');
            if (bannerIdEl) {
              Pandectes.fn.revokeConsent();
              bannerIdEl.classList.add('pandectes-override-show');
            }
          })
        }
      });
    }
    window.addEventListener('PandectesEvent_OnShow', function(event){
      pandectesClickBinder();
    });
    window.addEventListener('PandectesEvent_OnConsent', function(event) {
      if (firstConsent === false) {
        pandectesClickBinder();
        firstConsent = true;
      }
    }); 
    window.addEventListener('PandectesEvent_OnHide', function(event){
      bannerHasPerformed = true;
      const bannerIdEl = document.getElementById('pandectes-banner');
      if (bannerIdEl) {
        bannerIdEl.classList.remove('pandectes-override-show')
      }
    });
  </script> 

